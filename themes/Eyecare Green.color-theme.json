    { "name": "方法名-系统", "scope": ["support.function", "keyword.other.special-method"], "settings": { "foreground": "#c8a415" } },
    { "name": "字符串", "scope": ["string"], "settings": { "foreground": "#519657" } },
    { "name": "字符串-正则,`${res}`", "scope": ["string.regexp", "string.regexp keyword.operator", "string.regexp constant.character", "string.interpolated", "meta.template.expression.js punctuation.definition", "meta.template.expression.ts punctuation.definition"], "settings": { "foreground": "#e57373" } },
    { "name": "属性", "scope": ["entity.other.attribute-name"], "settings": { "foreground": "#ab47bc" } },
    { "name": "常量", "scope": ["constant", "constant.character", "constant.numeric", "constant.language"], "settings": { "foreground": "#c79100" } },
    { "name": "变量,php变量$,js模块属性,js对象属性,属性,json属性,html标签", "scope": ["variable", "variable.parameter", "punctuation.definition.variable", "punctuation.definition.variable.php", "punctuation.definition.variable.shell", "variable.interpolation", "support.type.object.module", "support.type.object.module.js", "support.type.object.module.ts", "meta.object-literal.key", "meta.object-literal.key.js", "meta.object-literal.key.ts", "support.type.property-name", "support.type.property-name.json", "entity.name.tag"], "settings": { "foreground": "#e57373" } },
